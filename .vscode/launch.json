  "version": "0.2.0",
  "configurations": [
    {
      "name": "Login",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/monzo",
      "args": [ "login" ],
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "Refresh Token",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/monzo",
      "args": [ "refresh-token" ],
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "whoami",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/monzo",
      "args": [ "whoami" ],
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "List Transactions",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/monzo",
      "args": [ "transactions", "get" ],
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "Get Transaction fake",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/monzo",
      "args": [ "transactions", "get", "fake" ],
      "envFile": "${workspaceFolder}/.env"
    }
  ]
}
